\newcommand{\handout}[5]{
  \noindent
  \begin{center}
  \framebox{
    \vbox{
            \hbox to 6.38in { {\bf{#1}} \hfill \em{#2} }
      \vspace{4mm}
            \hbox to 6.38in { {\Large \hfill \bf #5  \hfill} }
      \vspace{2mm}
            \hbox to 6.38in { {\bf{#3} \hfill \em{#4}} }
    }
  }
  \end{center}
  \vspace*{4mm}
}

\newcommand{\homework}[4]{\handout{#1}{#2}{#3}{#4}}


\newcommand{\roundhandout}[4]{
  \noindent
  \begin{center}
%  \framebox{
    \begin{tcolorbox}[colback=white]
    \vbox{
            \hbox to 6.38in {  \hfill #2 }
      \vspace{2mm}
            \hbox to 6.38in { {\bf \LARGE #1} \hfill #3 }
      \vspace{2mm}
            \hbox to 6.38in { \hfill #4 }
    }
    \end{tcolorbox}
%  }
  \end{center}
  \vspace*{4mm}
}

\newcommand{\roundhomework}[4]{\roundhandout{#1}{#2}{#3}{#4}}



