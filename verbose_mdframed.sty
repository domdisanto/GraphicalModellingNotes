%%%%%%%%% Starting New Counters %%%%%%%%%%%%%%
\newcounter{BoxQuestionCounter}
\newcounter{BoxPartCounter}[BoxQuestionCounter]

\newcommand{\boxsectionCount}[2][Question]{
 \stepcounter{BoxQuestionCounter}
    \begin{tcolorbox}
        \noindent\Large{\bf #1 \arabic{BoxQuestionCounter} #2}
    \end{tcolorbox}
    \vspace{2mm}
}

\newcommand{\boxsectionSkipCount}[1]{
    \begin{tcolorbox}
        \noindent\Large{\bf #1}
    \end{tcolorbox}
    \vspace{2mm}
}

\makeatletter 
\newcommand{\boxsection}{\@ifstar{\boxsectionSkipCount}{\boxsectionCount}}
\makeatother 


\newcommand{\boxsubsectionCount}[1]{
    \stepcounter{BoxPartCounter}
    \noindent{\bf  \arabic{BoxQuestionCounter}.\arabic{BoxPartCounter} #1} \newline 
    \vspace{-3mm}
    \hrulefill
    \vspace{4mm}
}


\newcommand{\boxsubsectionSkipCount}[1]{
    \noindent{\bf #1\hrulefill} 
    \vspace{6mm}
}


\makeatletter 
\newcommand{\boxsubsection}{\vspace{3mm}\@ifstar{\boxsubsectionSkipCount}{\boxsubsectionCount}}
\makeatother 


\newcommand{\AddCount}[1]{\stepcounter{#1}}

\newcommand{\boxsubsubsection}[1]{
    \vspace{\baselineskip}
    \noindent{\bf #1} 
    \vspace{3mm}
}


